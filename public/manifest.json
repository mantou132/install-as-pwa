{
  "name": "Install-As-PWA",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Convert any website into installable PWA",
  "web_accessible_resources": [
    "content.js"
  ],
  "permissions": [
    "debugger",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Convert to PWA"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
